extends ../layout
block body
block script
	script( defer type='module' src='/js/page/home.js')

	div(class='h-screen grid grid-cols-12')
		div(name='left-panel' class='bg-blue-500 text-slate-100 col-span-12 [&.active]:pointer-events-none ') 
			div(class='h-full w-4/5 mx-auto py-2 flex flex-col gap-2')
				img(src='/images/logo.png' class='w-24 h-24 border border-slate-50 rounded-full')
				p(class='font-light') Lorem ipsum dolor sit amet consectetur adipisicing elit. Tempora nulla architecto

				div(name='socket-id-container' class='mt-4 px-2 py-1 rounded-md bg-blue-400/30 border border-blue-400')
					p(class='mb-4 font-light') copy the personal code
					div(class='flex items-center justify-between')
						p(name='show-socket-id' class='font-medium') alskjdfalksjdfalskdjf 	
						button( name='socket-id-copy-button'
							class='p-0.5 border border-slate-200 text-slate-200 rounded-md'
							class='hover:border-slate-200/80 hover:text-slate-200/80 hover:bg-blue-400/60'
							class='active:border-slate-50 active:text-slate-50 active:bg-blue-400/90'
						)
							<svg class='w-4 h-4' xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><g fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5"><path d="M19.4 20H9.6a.6.6 0 0 1-.6-.6V9.6a.6.6 0 0 1 .6-.6h9.8a.6.6 0 0 1 .6.6v9.8a.6.6 0 0 1-.6.6"/><path d="M15 9V4.6a.6.6 0 0 0-.6-.6H4.6a.6.6 0 0 0-.6.6v9.8a.6.6 0 0 0 .6.6H9"/></g></svg>


				form(name='personal-code-container' class='mt-2')
					p(class='mb-1') Personal Code
					input(name='personal-code-input' type='text' placeholder='paste code here' value=''
						class='mb-2 px-2 py-1 w-full rounded-md outline-none border border-blue-400'
						class='text-slate-200 bg-blue-400/50 placeholder:text-slate-400'
						class='focus:border-blue-300'
					)

					div(class='grid grid-cols-2 gap-2')
						button(name='personal-chat-button' type='button' 
							class='flex gap-1 justify-center items-center px-1 py-1 border border-blue-400 rounded'
							class='hover:border-blue-300/80 hover:text-slate-100'
							class='active:border-blue-300 active:text-slate-50 active:bg-blue-400/50'
						)
							<svg class='pointer-events-none' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16"><path fill="currentColor" d="M2.678 11.894a1 1 0 0 1 .287.801a11 11 0 0 1-.398 2c1.395-.323 2.247-.697 2.634-.893a1 1 0 0 1 .71-.074A8 8 0 0 0 8 14c3.996 0 7-2.807 7-6s-3.004-6-7-6s-7 2.808-7 6c0 1.468.617 2.83 1.678 3.894m-.493 3.905a22 22 0 0 1-.713.129c-.2.032-.352-.176-.273-.362a10 10 0 0 0 .244-.637l.003-.01c.248-.72.45-1.548.524-2.319C.743 11.37 0 9.76 0 8c0-3.866 3.582-7 8-7s8 3.134 8 7s-3.582 7-8 7a9 9 0 0 1-2.347-.306c-.52.263-1.639.742-3.468 1.105"/></svg>
							span(class='pointer-events-none') chat

						button(name='personal-video-call-button' type='button' 
							class='flex gap-1 justify-center items-center px-1 py-1 border border-blue-400 rounded'
							class='hover:border-blue-300/80 hover:text-slate-100'
							class='active:border-blue-300 active:text-slate-50 active:bg-blue-400/50'
						)
							<svg class='w-6 h-6 pointer-events-none' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24"><path fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 12V8a2 2 0 0 0-2-2H5a2 2 0 0 0-2 2v8a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2zm0 0l4-4v8z"/></svg>
							span(class='pointer-events-none') video call


				form(name='stranger-code-container' class='mt-2')
					p(class='mb-1') Stranger Code
					div(class='grid grid-cols-2 gap-2')
						button(name='stranger-chat-button' type='button' 
							class='flex gap-1 justify-center items-center px-1 py-1 border border-blue-400 rounded'
							class='hover:border-blue-300/80 hover:text-slate-100'
							class='active:border-blue-300 active:text-slate-50 active:bg-blue-400/50'
						)
							<svg class='pointer-events-none' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16"><path fill="currentColor" d="M2.678 11.894a1 1 0 0 1 .287.801a11 11 0 0 1-.398 2c1.395-.323 2.247-.697 2.634-.893a1 1 0 0 1 .71-.074A8 8 0 0 0 8 14c3.996 0 7-2.807 7-6s-3.004-6-7-6s-7 2.808-7 6c0 1.468.617 2.83 1.678 3.894m-.493 3.905a22 22 0 0 1-.713.129c-.2.032-.352-.176-.273-.362a10 10 0 0 0 .244-.637l.003-.01c.248-.72.45-1.548.524-2.319C.743 11.37 0 9.76 0 8c0-3.866 3.582-7 8-7s8 3.134 8 7s-3.582 7-8 7a9 9 0 0 1-2.347-.306c-.52.263-1.639.742-3.468 1.105"/></svg>
							span(class='pointer-events-none') chat

						button(name='stranger-video-call-button' type='button' 
							class='flex gap-1 justify-center items-center px-1 py-1 border border-blue-400 rounded'
							class='hover:border-blue-300/80 hover:text-slate-100'
							class='active:border-blue-300 active:text-slate-50 active:bg-blue-400/50'
						)
							<svg class='w-6 h-6 pointer-events-none' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24"><path fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 12V8a2 2 0 0 0-2-2H5a2 2 0 0 0-2 2v8a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2zm0 0l4-4v8z"/></svg>
							span(class='pointer-events-none') video call

				div(class='flex-1 mt-4 flex items-end ')
					div(name='allow-from-stranger' class='flex items-center gap-1')
						input(type='checkbox' id='checkbox' class='w-4 h-4 cursor-pointer') 
						label(for='checkbox' class='cursor-pointer') allow from stranger


		//- div(class='bg-slate-50 col-span-12') 
			div(class='h-full border border-red-300')
				div(class='relative aspect-video border border-slate-300 bg-blue-400')
					div(class='absolute w-40 aspect-video border border-blue-300 ')
						video(name='local-video' src="/videos/video.mp4" muted hidden) 

					video(name='remote-video' src="/videos/video.mp4" hidden) 

					input(type='checkbox' id='call-button' class='peer/call' hidden)
					div(name='recording-controller' class='hidden peer-checked/call:flex absolute right-3 top-3 bg-slate-700/60 text-slate-50 px-3 py-1.5 rounded-md gap-2 items-center')
						input(type='checkbox' id='play-pause' class='peer/play-pause hidden')
						label(for='play-pause'
							class='cursor-pointer '
							class='peer-checked/play-pause:[&>*:first-child]:hidden [&>*:first-child]:block'
							class='peer-checked/play-pause:[&>*:nth-child(2)]:block [&>*:nth-child(2)]:hidden'
						)
							<svg class='block' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24"><path fill="currentColor" d="M6 19h4V5H6zm8-14v14h4V5z"/></svg>
							<svg class='hidden' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16"><path fill="currentColor" d="m11.596 8.697l-6.363 3.692c-.54.313-1.233-.066-1.233-.697V4.308c0-.63.692-1.01 1.233-.696l6.363 3.692a.802.802 0 0 1 0 1.393"/></svg>

						label(for='call-button' class='cursor-pointer text-sm') Recording

					//- 
						if we add .called class  in this container then other icons will be hidden
						if we add .active class  to any child of this container then they will be red color
					div(name='buttons-container' 
						class='absolute bottom-4 left-0 w-full flex gap-2 justify-center items-center '
						class='[&.called_.called]:hidden'
						class='[&_.active]:bg-red-500 [&_.active]:scale-125 [&_.active:active]:bg-red-600 '
						class='peer-checked/call:[&>*:not(.active)]:hidden'
						)

						input(type='checkbox' id='microphone-on-off' class='peer/microphone hidden')
						label(for='microphone-on-off'
							class='cursor-pointer p-2 rounded-full bg-slate-700/60 text-white hover:bg-slate-700/70 active:bg-slate-700/90 hover:scale-125 transition-transform duration-200'
							class='called'
							class='peer-checked/microphone:bg-slate-700/90 '
							class='peer-checked/microphone:[&>*:first-child]:hidden [&>*:first-child]:block'
							class='peer-checked/microphone:[&>*:nth-child(2)]:block [&>*:nth-child(2)]:hidden'
						)
							<svg class='block' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 1024 1024"><path fill="currentColor" d="M512 128a128 128 0 0 0-128 128v256a128 128 0 1 0 256 0V256a128 128 0 0 0-128-128m0-64a192 192 0 0 1 192 192v256a192 192 0 1 1-384 0V256A192 192 0 0 1 512 64m-32 832v-64a288 288 0 0 1-288-288v-32a32 32 0 0 1 64 0v32a224 224 0 0 0 224 224h64a224 224 0 0 0 224-224v-32a32 32 0 1 1 64 0v32a288 288 0 0 1-288 288v64h64a32 32 0 1 1 0 64H416a32 32 0 1 1 0-64z"/></svg>
							<svg class='hidden' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 1024 1024"><path fill="currentColor" d="m412.16 592.128l-45.44 45.44A191.232 191.232 0 0 1 320 512V256a192 192 0 1 1 384 0v44.352l-64 64V256a128 128 0 1 0-256 0v256c0 30.336 10.56 58.24 28.16 80.128m51.968 38.592A128 128 0 0 0 640 512v-57.152l64-64V512a192 192 0 0 1-287.68 166.528zM314.88 779.968l46.144-46.08A222.976 222.976 0 0 0 480 768h64a224 224 0 0 0 224-224v-32a32 32 0 1 1 64 0v32a288 288 0 0 1-288 288v64h64a32 32 0 1 1 0 64H416a32 32 0 1 1 0-64h64v-64c-61.44 0-118.4-19.2-165.12-52.032M266.752 737.6A286.976 286.976 0 0 1 192 544v-32a32 32 0 0 1 64 0v32c0 56.832 21.184 108.8 56.064 148.288z"/><path fill="currentColor" d="M150.72 859.072a32 32 0 0 1-45.44-45.056l704-708.544a32 32 0 0 1 45.44 45.056z"/></svg>

						input(type='checkbox' id='camera-on-off' class='peer/camera hidden')
						label(for='camera-on-off'
							class='cursor-pointer p-2 rounded-full bg-slate-700/60 text-white hover:bg-slate-700/70 active:bg-slate-700/90 hover:scale-125 transition-transform duration-200'
							class='called'
							class='peer-checked/camera:bg-slate-700/90 '
							class='peer-checked/camera:[&>*:first-child]:hidden [&>*:first-child]:block'
							class='peer-checked/camera:[&>*:nth-child(2)]:block [&>*:nth-child(2)]:hidden'
						)
							<svg class='block' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24"><path fill="currentColor" d="M20 4h-3.17L15 2H9L7.17 4H4a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h16a2 2 0 0 0 2-2V6a2 2 0 0 0-2-2m0 14H4V6h4.05l1.83-2h4.24l1.83 2H20zM12 7a5 5 0 0 0-5 5a5 5 0 0 0 5 5a5 5 0 0 0 5-5a5 5 0 0 0-5-5m0 8a3 3 0 0 1-3-3a3 3 0 0 1 3-3a3 3 0 0 1 3 3a3 3 0 0 1-3 3"/></svg>
							<svg class='hidden' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24"><path fill="currentColor" d="M17 12c0 .54-.1 1.05-.26 1.54L15 11.78A3.057 3.057 0 0 0 12.22 9l-1.76-1.74c.49-.16 1-.26 1.54-.26c2.76 0 5 2.24 5 5M9.88 4h4.24l1.83 2H20v10.8l1.88 1.88c.08-.21.12-.44.12-.68V6c0-1.11-.89-2-2-2h-3.17L15 2H9L7.18 4L8.6 5.4zm12.23 17.46l-1.27 1.27L18.11 20H4a2 2 0 0 1-2-2V6c0-.58.25-1.1.65-1.46L1.11 3l1.28-1.27zM9 12c0 1.66 1.34 3 3 3c.33 0 .65-.07.94-.17l-3.77-3.77c-.1.3-.17.61-.17.94m7.11 6l-1.66-1.66c-.73.41-1.56.66-2.45.66c-2.76 0-5-2.24-5-5c0-.89.25-1.72.66-2.45L4.11 6H4v12z"/></svg>


						label(name='call' for='call-button'
							class='cursor-pointer p-2 rounded-full bg-slate-700/60 text-white hover:bg-slate-700/70 active:bg-slate-700/90 hover:scale-125 transition-transform duration-200'
							class='mx-1 active'
						)
							<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24"><path fill="currentColor" d="M4.025 17L.4 13.475l1-1.025q2.175-2.225 4.963-3.337T12 8q2.85 0 5.625 1.113T22.6 12.45l1 1.025L19.975 17L16 14v-3.35q-.95-.3-1.95-.475T12 10q-1.05 0-2.05.175T8 10.65V14z"/></svg>

						input(type='checkbox' id='flip-camera' class='peer/flip-camera hidden')
						label(for='flip-camera'
							class='cursor-pointer p-2 rounded-full bg-slate-700/60 text-white hover:bg-slate-700/70 active:bg-slate-700/90 hover:scale-125 transition-transform duration-200'
							class='called'
							class='peer-checked/flip-camera:bg-slate-700/90 '
						)
							<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 48 48"><g fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="4"><path d="M12 11h5l2-4h10l2 4h5v16H12z"/><circle cx="24" cy="18" r="4"/><path d="M24 38C12.954 38 4 33.523 4 28c0-1.422.594-2.775 1.664-4M24 38l-4-4m4 4l-4 4m12-4.832C39.064 35.625 44 32.1 44 28c0-1.422-.594-2.775-1.664-4"/></g></svg>

						input(type='checkbox' id='recording' class='peer/recording hidden')
						label(for='recording'
							class='cursor-pointer p-2 rounded-full bg-slate-700/60 text-white hover:bg-slate-700/70 active:bg-slate-700/90 hover:scale-125 transition-transform duration-200'
							class='called'
							class='peer-checked/recording:bg-slate-700/90 '
							class='peer-checked/recording:[&>*:first-child]:hidden [&>*:first-child]:block'
							class='peer-checked/recording:[&>*:nth-child(2)]:block [&>*:nth-child(2)]:hidden'
						)
							<svg class='block' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 32 32"><path fill="currentColor" d="M16 2C8.3 2 2 8.3 2 16s6.3 14 14 14s14-6.3 14-14S23.7 2 16 2m0 20c-3.3 0-6-2.7-6-6s2.7-6 6-6s6 2.7 6 6s-2.7 6-6 6"/></svg>
							<svg class='hidden text-red-500' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 256 256"><circle cx="127" cy="129" r="81" fill="currentColor" fill-rule="evenodd"/></svg>

		//- div(name='right-section' class=' bg-slate-50 col-span-3 ') 
			div(class='h-screen flex flex-col px-4 pt-2')
				div(name='right-top' class='flex-1 overflow-y-scroll')
					div( name='their-message' class='flex items-start gap-2 mb-2')
						img(src='/images/logo.png' alt='their avatar' class='w-6 h-6 rounded-full')
						div(class='max-w-[80%] px-2 py-1 bg-slate-200 border border-slate-300 rounded-md rounded-tl-none') 
							p(class='text-slate-600') Lorem ipsum dolor sit amet 

					div( name='your-message' class='flex items-end gap-2 mt-2')
						div(class='max-w-[80%] ml-auto px-2 py-1 bg-blue-200 border border-blue-300 rounded-md rounded-br-none') 
							p(class='text-blue-950/80') Lorem ipsum dolor sit, amet consectetur 

						span(class='text-blue-500')
							<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24"><path fill="currentColor" d="M21 7L9 19l-5.5-5.5l1.41-1.41L9 16.17L19.59 5.59z"/></svg>
							//- <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24"><path fill="currentColor" d="M.41 13.41L6 19l1.41-1.42L1.83 12m20.41-6.42L11.66 16.17L7.5 12l-1.43 1.41L11.66 19l12-12M18 7l-1.41-1.42l-6.35 6.35l1.42 1.41z"/></svg>


				div(name='right-bottom' class='my-4 flex justify-end items-center')
					input(type='text' placeholder='your message ...' 
						class='w-full relative pl-3 pr-10 py-1 rounded-md border border-slate-300 '
						class='outline-none focus:border-slate-400'
						class='text-slate-700'
					)
					button(
						class='absolute p-2 bg-slate-200 hover:bg-slate-300/60'
						class='active:bg-slate-300/90 active:text-blue-700'
					)
						<svg xmlns="http://www.w3.org/2000/svg" width="15" height="16" viewBox="0 0 15 16"><path fill="currentColor" d="M12.49 7.14L3.44 2.27c-.76-.41-1.64.3-1.4 1.13l1.24 4.34c.05.18.05.36 0 .54l-1.24 4.34c-.24.83.64 1.54 1.4 1.13l9.05-4.87a.98.98 0 0 0 0-1.72Z"/></svg>
	
	//- dialog(name='incomming-call-dialog' class='[&::backdrop]:backdrop-blur-sm bg-transparent ')
	div(name='incomming-call-dialog' class='hidden backdrop-blur-sm h-screen fixed inset-0 justify-center items-center bg-blue-300/20')
		div( class=' p-8 flex flex-col gap-4 items-center bg-slate-50 rounded-md border border-slate-300 shadow-sm')
			p(name='dialog-title' class='font-medium text-slate-800 capitalize') incomming chat call

			div(class='w-40 h-40 rounded-full bg-blue-600 p-8 text-slate-100')
				<svg class='w-24 h-24' xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 256 256"><path fill="currentColor" d="M230.93 220a8 8 0 0 1-6.93 4H32a8 8 0 0 1-6.92-12c15.23-26.33 38.7-45.21 66.09-54.16a72 72 0 1 1 73.66 0c27.39 8.95 50.86 27.83 66.09 54.16a8 8 0 0 1 .01 8"/></svg>
			
			div(name='button-section' class='mt-2 flex gap-4 items-center')
				button(name='call-in-button' class='flex gap-2 items-center'
					class='px-3 py-1.5 border border-blue-400 rounded-md text-blue-600'
					class='hover:text-blue-700 hover:border-blue-500 hover:bg-blue-50'
					class='active:text-blue-800 active:border-blue-600 active:bg-blue-100'
				)
					<svg class='pointer-events-none' xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 1024 1024"><path fill="currentColor" d="M1000.35 771.616c-15.664-11.008-187.059-124.974-208.066-137.806c-9.152-5.6-20.32-8.336-32.464-8.336c-15.664 0-33.008 4.56-49.935 13.472c-16.497 8.688-66.464 37.12-90.913 51.088c-21.536-15.12-72.127-53.872-151.84-133.664c-79.215-79.184-118.319-130.191-133.535-151.84c13.936-24.432 42.336-74.4 50.976-90.848c16.368-31.008 18.225-61.968 4.944-82.848c-12.463-19.68-125.968-191.808-137.68-208C240.813 7.554 221.021-.702 199.55-.702c-16.944 0-34.912 5.12-50.496 15.968C147.55 16.338-3.043 125.522-1.043 187.618C4.51 362.05 174.013 567.042 315.228 708.306s346.224 310.815 521.344 316.399h1.44c61.312 0 169.089-148.688 170.129-150.16c28.272-40.4 17.968-84.88-7.791-102.929m-44.209 65.651c-40.592 56.224-98.225 114.945-120.784 123.346c-120.032-5.632-288.464-111.12-474.88-297.568c-186.4-186.464-291.872-354.704-297.44-474.336c8.096-22.624 66.815-80.624 122.527-120.912c4.128-2.848 9.216-4.496 13.968-4.496c1.055 0 1.935.096 2.624.224c18 26.16 114.624 172.433 132.16 199.776c.064 2.88-.911 10.19-6.4 20.623c-5.84 11.12-24.032 43.536-49.904 88.88l-20.128 35.28l23.344 33.248c17.408 24.72 58.816 78.464 140.624 160.288c82.16 82.192 135.712 123.473 160.336 140.784l33.248 23.344l35.28-20.16c34.193-19.537 75.504-42.945 88.945-50c10.784-5.68 18.16-6.129 20.16-6.129c.32 0 .593 0 .816.033c24.496 15.376 173.937 114.592 200.32 132.688c.432 2.56.031 8.128-4.816 15.088zm-312.305-460.75c4.128 4.176 9.938 6.722 16.386 6.546l11.712-.273c.223 0 .383-.095.64-.11l229.503 1.007c12.912-.304 23.616-10.992 23.92-23.937l.016-16.416c-1.952-15.232-13.937-24.16-26.865-23.872l-151.504-.4l261.952-261.6c12.497-12.496 12.497-32.769 0-45.265c-12.496-12.48-32.752-12.48-45.248 0l-262.672 262.32l.88-154.833c.288-12.927-9.967-24.191-22.895-23.887l-16.416.015c-12.96.32-23.664 8.017-23.937 20.945l-.656 231.008c0 .223.88.383.88.607l-1.28 11.712c-.128 6.496 1.391 12.272 5.584 16.433"/></svg>
					span(class='pointer-events-none') Accept

				button(name='call-off-button' class='flex gap-2 items-center'
